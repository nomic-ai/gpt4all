{
  "name": "gpt4all",
  "version": "2.0.0",
  "packageManager": "yarn@3.5.1",
  "main": "src/gpt4all.js",
  "repository": "nomic-ai/gpt4all",
  "scripts": {
    "test": "jest",
    "build:backend": "node scripts/build.js",
    "install": "node-gyp-build",
    "prebuild": "node scripts/prebuild.js",
    "predocs:build": "node scripts/docs.js",
    "docs:build": "documentation readme ./src/gpt4all.d.ts --parse-extension d.ts --format md --section documentation --readme-file docs/api.md",
    "postdocs:build": "node scripts/docs.js"
  },
  "dependencies": {
    "md5-file": "^5.0.0",
    "mkdirp": "^3.0.1",
    "node-addon-api": "^6.1.0",
    "node-gyp-build": "^4.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.1.5",
    "documentation": "^14.0.2",
    "jest": "^29.5.0",
    "prebuildify": "^5.0.1",
    "prettier": "^2.8.8"
  },
  "engines": {
    "node": ">= 18.x.x"
  },
  "prettier": {
    "endOfLine": "lf",
    "tabWidth": 4
  },
  "jest": {
    "verbose": true
  }
}
